<% if notice %>
    <p id="notice"><%= notice %></p>
<% end %>

<h1>Listing Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Address</th>
      <th>Phone</th>
